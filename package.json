{
  "name": "@lit-jsdocs",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "analyze": "custom-elements-manifest analyze --config ./custom-elements.config.js",
    "analyze:dev": "wireit",
    "docs:dev": "wireit",
    "docs:build": "vitepress build",
    "docs:preview": "vitepress preview",
    "tsc:dev": "wireit",
    "tsc:build": "wireit"
  },
  "wireit": {
    "docs:dev": {
      "command": "vitepress dev",
      "service": true,
      "dependencies": [
        "analyze:dev",
        "tsc:dev"
      ]
    },
    "analyze:dev": {
      "command": "custom-elements-manifest analyze --config ./custom-elements.config.js --watch",
      "service": true
    },
    "tsc:dev": {
      "command": "tsc -w",
      "service": true
    },
    "tsc:build": {
      "command": "tsc"
    }
  },
  "dependencies": {
    "@blockquote/cem-to-markdown-readme": "^1.0.0",
    "@github/cem-plugin-readme": "^0.0.1",
    "cem-plugin-custom-jsdoc-tags": "^1.0.2",
    "cem-plugin-expanded-types": "^1.2.0",
    "cem-plugin-jsdoc-example": "^0.0.9",
    "cem-plugin-module-file-extensions": "^0.0.5",
    "cem-plugin-reactify": "^0.0.2",
    "comment-parser": "^1.4.1",
    "custom-elements-manifest-inheritance": "^1.0.0",
    "ejs": "^3.1.10",
    "graceful-fs": "^4.2.11",
    "highlight.js": "^11.9.0",
    "jsdom": "^24.0.0",
    "lit": "^3.1.2",
    "prettier": "^3.2.5",
    "vite-plugin-web-components-hmr": "^0.1.3",
    "vue-highlight-code": "^0.2.0",
    "vue-live": "^2.5.4",
    "wireit": "^0.14.4",
    "xmlserializer": "^0.6.1"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.9.8",
    "fast-glob": "^3.3.2",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vitepress": "^1.1.4"
  }
}
